title:"\u092f\u0926\u093f \u0915\u094b\u0908 \u092a\u0939\u091a\u093e\u0928 \u0935\u093f\u0935\u0930\u0923 (\u0928\u093e\u092e, \u0932\u093f\u0902\u0917 \u092f\u093e \u091c\u0928\u094d\u092e \u0924\u093f\u0925\u093f) \u092e\u0947\u0930\u0947 \u0935\u093e\u0938\u094d\u0924\u0935\u093f\u0915 \u092a\u0939\u091a\u093e\u0928 \u0935\u093f\u0935\u0930\u0923 \u0938\u0947 \u092e\u0947\u0932 \u0928\u0939\u0940\u0902 \u0916\u093e\u0924\u093e \u0939\u0948, \u0924\u094b \u092e\u0941\u091d\u0947 \u0915\u094d\u092f\u093e \u0915\u0930\u0928\u093e \u091a\u093e\u0939\u093f\u090f?",content:"\u0915\u0943\u092a\u092f\u093e https://uidai.gov.in/images/commdoc/26_JAN_2023_Aadhaar_List_of_documents_English.pdf \u092a\u0930 \u0938\u0942\u091a\u0940\u092c\u0926\u094d\u0927 \u092e\u0942\u0932 \u092a\u0939\u091a\u093e\u0928 \u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u094b\u0902 \u0915\u0947 \u0938\u093e\u0925 \u0906\u0927\u093e\u0930 \u0915\u0947\u0902\u0926\u094d\u0930 \u092a\u0930 \u091c\u093e\u090f\u0902\u0964"},seven:{title:"\u092f\u0926\u093f \u092e\u0948\u0902 \u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u093c \u0911\u092b\u093c\u0932\u093e\u0907\u0928 \u091c\u092e\u093e \u0915\u0930\u0928\u093e \u091a\u093e\u0939\u0924\u093e \u0939\u0942\u0902, \u0924\u094b \u092e\u0948\u0902 \u0906\u0927\u093e\u0930 \u0915\u0947\u0902\u0926\u094d\u0930 \u0915\u0948\u0938\u0947 \u0922\u0942\u0902\u0922 \u0938\u0915\u0924\u093e \u0939\u0942\u0902?",content:{list_1:"(1) \u0915\u0943\u092a\u092f\u093e https://shuvan.nrsc.gov.in/aadhaar/ \u092a\u0930 \u091c\u093e\u090f\u0902",list_2:"(2) \u0906\u0938-\u092a\u093e\u0938 \u0915\u0947 \u0906\u0927\u093e\u0930 \u0915\u0947\u0902\u0926\u094d\u0930\u094b\u0902 \u0915\u093e \u092a\u0924\u093e \u0932\u0917\u093e\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f, '\u0906\u0938-\u092a\u093e\u0938 \u0915\u0947\u0902\u0926\u094d\u0930' \u091f\u0948\u092c \u092a\u0930 \u0915\u094d\u0932\u093f\u0915 \u0915\u0930\u0947\u0902\u0964 \u0906\u0938-\u092a\u093e\u0938 \u0915\u0947 \u0906\u0927\u093e\u0930 \u0915\u0947\u0902\u0926\u094d\u0930 \u0926\u0947\u0916\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0905\u092a\u0928\u093e \u0938\u094d\u0925\u093e\u0928 \u0935\u093f\u0935\u0930\u0923 \u0926\u0930\u094d\u091c \u0915\u0930\u0947\u0902\u0964",list_3:"(3) \u0905\u092a\u0928\u0947 \u092a\u093f\u0928 \u0915\u094b\u0921 \u0915\u094d\u0937\u0947\u0924\u094d\u0930 \u0915\u0947 \u092d\u0940\u0924\u0930 \u0906\u0927\u093e\u0930 \u0915\u0947\u0902\u0926\u094d\u0930\u094b\u0902 \u0915\u093e \u092a\u0924\u093e \u0932\u0917\u093e\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f, '\u092a\u093f\u0928 \u0915\u094b\u0921 \u0926\u094d\u0935\u093e\u0930\u093e \u0916\u094b\u091c\u0947\u0902' \u091f\u0948\u092c \u092a\u0930 \u0915\u094d\u0932\u093f\u0915 \u0915\u0930\u0947\u0902\u0964 \u0909\u0938 \u0915\u094d\u0937\u0947\u0924\u094d\u0930 \u092e\u0947\u0902 \u0906\u0927\u093e\u0930 \u0915\u0947\u0902\u0926\u094d\u0930 \u0926\u0947\u0916\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0905\u092a\u0928\u0947 \u0915\u094d\u0937\u0947\u0924\u094d\u0930 \u0915\u093e \u092a\u093f\u0928 \u0915\u094b\u0921 \u0926\u0930\u094d\u091c \u0915\u0930\u0947\u0902\u0964"}},eight:{title:"\u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u093c \u0938\u092c\u092e\u093f\u091f \u0915\u0930\u0928\u0947 \u0915\u093e \u0936\u0941\u0932\u094d\u0915 \u0915\u094d\u092f\u093e \u0939\u0948?",content:"14 \u0938\u093f\u0924\u0902\u092c\u0930 2023 \u0924\u0915 \u092e\u093e\u092f\u0906\u0927\u093e\u0930 \u092a\u094b\u0930\u094d\u091f\u0932 \u092a\u0930 \u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u093c \u091c\u092e\u093e \u0915\u0930\u0928\u093e \u0928\u093f\u0903\u0936\u0941\u0932\u094d\u0915 \u0939\u0948\u0964 \u0906\u0927\u093e\u0930 \u0915\u0947\u0902\u0926\u094d\u0930 \u092a\u0930 \u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u093c \u091c\u092e\u093e \u0915\u0930\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f, \u0932\u093e\u0917\u0942 \u0936\u0941\u0932\u094d\u0915 \u0930\u0941. 50 \u0939\u0948\u0964"},nine:{title:"\u092e\u0941\u091d\u0947 \u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u093c \u0915\u092c \u0924\u0915 \u0938\u092c\u092e\u093f\u091f \u0915\u0930\u0928\u0947 \u091a\u093e\u0939\u093f\u090f?",content:"\u091c\u0940\u0935\u0928\u092f\u093e\u092a\u0928 \u092e\u0947\u0902 \u0906\u0938\u093e\u0928\u0940, \u092c\u0947\u0939\u0924\u0930 \u0938\u0947\u0935\u093e \u0935\u093f\u0924\u0930\u0923 \u0914\u0930 \u0938\u091f\u0940\u0915 \u092a\u094d\u0930\u092e\u093e\u0923\u0940\u0915\u0930\u0923 \u0915\u094b \u0938\u0915\u094d\u0937\u092e \u0915\u0930\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u093c\u094b\u0902 \u0915\u094b \u0905\u0926\u094d\u092f\u0924\u0928 \u0915\u0930\u0928\u093e \u0935\u093e\u0902\u091b\u0928\u0940\u092f \u0939\u0948\u0964 \u0907\u0938\u0932\u093f\u090f, \u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u093c\u094b\u0902 \u0915\u094b \u0936\u0940\u0918\u094d\u0930\u0924\u093e \u0938\u0947 \u091c\u092e\u093e \u0915\u0930\u0928\u093e \u0906\u092a\u0915\u0947 \u0939\u093f\u0924 \u092e\u0947\u0902 \u0939\u0948\u0964"},ten:{title:"\u092e\u0948\u0902 \u090f\u0915 \u0905\u0928\u093f\u0935\u093e\u0938\u0940 \u092d\u093e\u0930\u0924\u0940\u092f (\u090f\u0928\u0906\u0930\u0906\u0908) \u0939\u0942\u0902\u0964 \u092e\u0948\u0902 \u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u093c \u0915\u0948\u0938\u0947 \u0938\u092c\u092e\u093f\u091f \u0915\u0930 \u0938\u0915\u0924\u093e \u0939\u0942\u0901?",content:"\u091c\u092c \u092d\u0940 \u0906\u092a \u092d\u093e\u0930\u0924 \u092e\u0947\u0902 \u0939\u094b\u0902 \u0924\u094b \u0906\u092a \u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u093c \u0938\u092c\u092e\u093f\u091f \u0915\u0930 \u0938\u0915\u0924\u0947 \u0939\u0948\u0902\u0964"}}},aadharUpdateHistory:{title1:"\u0905\u092a\u0928\u093e \u0906\u0927\u093e\u0930 \u0905\u092a\u0921\u0947\u091f \u0907\u0924\u093f\u0939\u093e\u0938 \u091c\u093e\u0902\u091a\u0947\u0902",label1:"\u0905\u092d\u093f\u0932\u0947\u0916",label2:"\u0928\u093e\u092e\u093e\u0902\u0915\u0928/\u0905\u0926\u094d\u092f\u0924\u0928 \u0915\u0940 \u0924\u093f\u0925\u093f",label3:"URN/EID",label4:"\u092a\u094d\u0930\u0915\u093e\u0930",list1:"\u0928\u093e\u092e",list2:"\u092e\u094b\u092c\u093e\u0907\u0932",list3:"\u0908\u092e\u0947\u0932",list4:"\u091c\u0928\u094d\u092e \u0924\u093f\u0925\u093f",list5:"\u0932\u093f\u0902\u0917",list6:"\u092a\u0924\u093e"},ssup:{howItWorks:{title:"\u092f\u0939 \u0915\u093f\u0938 \u092a\u094d\u0930\u0915\u093e\u0930 \u0915\u093e\u092e \u0915\u0930\u0924\u093e \u0939\u0948?",description:"\u0911\u0928\u0932\u093e\u0907\u0928 \u0905\u0926\u094d\u092f\u0924\u0928 \u092a\u094d\u0930\u0915\u094d\u0930\u093f\u092f\u093e \u0915\u094b \u0938\u092e\u091d\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0915\u0943\u092a\u092f\u093e \u0928\u093f\u092e\u094d\u0928\u0932\u093f\u0916\u093f\u0924 \u091a\u0930\u0923\u094b\u0902 \u0915\u094b \u092a\u0922\u093c\u0947\u0902\u0964 \u0905\u0928\u0941\u0930\u094b\u0927 \u0915\u094b \u0938\u0902\u0938\u093e\u0927\u093f\u0924 \u0915\u0930\u0928\u0947 \u092f\u093e \u0905\u0938\u094d\u0935\u0940\u0915\u093e\u0930 \u0915\u0930\u0928\u0947 \u092e\u0947\u0902 \u0915\u093f\u0938\u0940 \u092d\u0940 \u0926\u0947\u0930\u0940 \u0938\u0947 \u092c\u091a\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0935\u093f\u092d\u093f\u0928\u094d\u0928 \u091a\u0930\u0923\u094b\u0902 \u092e\u0947\u0902 \u0938\u0941\u091d\u093e\u090f \u0917\u090f \u0915\u093e\u0930\u094d\u092f\u094b\u0902 \u0915\u093e \u092a\u093e\u0932\u0928 \u0915\u0930\u0947\u0902\u0964",proceedToUpdateAadhaar:"\u0906\u0927\u093e\u0930 \u0905\u0926\u094d\u092f\u0924\u0928 \u0915\u0930\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0906\u0917\u0947 \u092c\u0922\u093c\u0947\u0902\u0964",stepDescription:{one:"\u0938\u0942\u091a\u0940 \u0938\u0947 \u0905\u0926\u094d\u092f\u0924\u0928 \u0915\u0930\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u092a\u0924\u093e \u092b\u093c\u0940\u0932\u094d\u0921 \u0915\u093e \u091a\u092f\u0928 \u0915\u0930\u0947\u0902\u0964 \u0907\u0938 \u0911\u0928\u0932\u093e\u0907\u0928 \u092a\u094b\u0930\u094d\u091f\u0932 \u0915\u0947 \u091c\u0930\u093f\u090f \u0906\u092a \u092a\u0924\u093e \u0905\u092a\u0921\u0947\u091f \u0915\u0930 \u0938\u0915\u0924\u0947 \u0939\u0948\u0902\u0964 \u0928\u093e\u092e, \u0932\u093f\u0902\u0917, \u091c\u0928\u094d\u092e \u0924\u093f\u0925\u093f, \u092e\u094b\u092c\u093e\u0907\u0932 \u0928\u0902\u092c\u0930, \u0908\u092e\u0947\u0932 \u0914\u0930 \u092c\u093e\u092f\u094b\u092e\u0947\u091f\u094d\u0930\u093f\u0915\u094d\u0938 \u0921\u0947\u091f\u093e \u0915\u0947 \u0932\u093f\u090f, \u0915\u0943\u092a\u092f\u093e \u0928\u093f\u0915\u091f\u0924\u092e \u0906\u0927\u093e\u0930 \u0938\u0947\u0935\u093e \u0915\u0947\u0902\u0926\u094d\u0930 \u092a\u0930 \u091c\u093e\u090f\u0901\u0964",two:"\u0915\u0943\u092a\u092f\u093e \u0906\u0935\u0936\u094d\u092f\u0915 \u091c\u0928\u0938\u093e\u0902\u0916\u094d\u092f\u093f\u0915\u0940\u092f \u0921\u0947\u091f\u093e \u0926\u0930\u094d\u091c \u0915\u0930\u0947\u0902\u0964 \u0915\u0941\u091b \u091c\u0928\u0938\u093e\u0902\u0916\u094d\u092f\u093f\u0915\u0940\u092f \u0921\u0947\u091f\u093e \u0915\u094b \u0921\u094d\u0930\u0949\u092a\u0921\u093e\u0909\u0928 \u0938\u0942\u091a\u0940 \u0938\u0947 \u091a\u092f\u0928 \u0915\u0940 \u0906\u0935\u0936\u094d\u092f\u0915\u0924\u093e \u0939\u094b\u0924\u0940 \u0939\u0948\u0964 \u091c\u0928\u0938\u093e\u0902\u0916\u094d\u092f\u093f\u0915\u0940 \u0921\u0947\u091f\u093e \u0926\u0930\u094d\u091c \u0915\u0930\u0924\u0947 \u0938\u092e\u092f \u092f\u093e \u0921\u094d\u0930\u0949\u092a\u0921\u093e\u0909\u0928 \u0935\u093f\u0915\u0932\u094d\u092a\u094b\u0902 \u092e\u0947\u0902 \u0938\u0947 \u091a\u092f\u0928 \u0915\u0930\u0924\u0947 \u0938\u092e\u092f \u0915\u0943\u092a\u092f\u093e \u0938\u093e\u0935\u0927\u093e\u0928 \u0930\u0939\u0947\u0902\u0964",three:"\u092a\u0924\u093e \u0905\u092a\u0921\u0947\u091f \u0915\u0930\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u092e\u0942\u0932 \u0938\u0939\u093e\u092f\u0915 \u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u093c \u0915\u0940 \u0938\u094d\u0915\u0948\u0928 \u0915\u0949\u092a\u0940 \u0915\u0940 \u0906\u0935\u0936\u094d\u092f\u0915\u0924\u093e \u0939\u094b\u0924\u0940 \u0939\u0948\u0964 \u0915\u0943\u092a\u092f\u093e \u0938\u0941\u0928\u093f\u0936\u094d\u091a\u093f\u0924 \u0915\u0930\u0947\u0902 \u0915\u093f \u0938\u0939\u093e\u092f\u0915 \u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u093c \u0906\u092a\u0915\u0947 \u0928\u093e\u092e \u092a\u0930 \u0939\u0948 \u0914\u0930 \u092e\u093e\u0928\u094d\u092f \u0939\u0948\u0964 \u092f\u0926\u093f \u0906\u092a \u092f\u0942\u0906\u0908\u0921\u0940\u090f\u0906\u0908 \u092e\u093e\u0928\u0915 \u092a\u094d\u0930\u093e\u0930\u0942\u092a \u092e\u0947\u0902 \u090f\u0915 \u092a\u094d\u0930\u092e\u093e\u0923 \u092a\u0924\u094d\u0930 \u092a\u094d\u0930\u0926\u093e\u0928 \u0915\u0930 \u0930\u0939\u0947 \u0939\u0948\u0902, \u0924\u094b \u0905\u0928\u0941\u0930\u094b\u0927 \u0915\u0940 \u0905\u0938\u094d\u0935\u0940\u0915\u0943\u0924\u093f \u0938\u0947 \u092c\u091a\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0915\u0943\u092a\u092f\u093e \u091c\u093e\u0930\u0940 \u0915\u0930\u0928\u0947 \u0915\u0940 \u0924\u093e\u0930\u0940\u0916, \u0915\u094d\u0930\u0949\u0938\u094d\u0921 \u0938\u093e\u0907\u0928, \u092b\u094b\u091f\u094b \u092a\u0930 \u0938\u094d\u091f\u093e\u092e\u094d\u092a \u0914\u0930 \u0915\u094b\u0908 \u0913\u0935\u0930\u0930\u093e\u0907\u091f\u093f\u0902\u0917 \u091c\u0948\u0938\u0940 \u0938\u093e\u092e\u093e\u0928\u094d\u092f \u0917\u0932\u0924\u093f\u092f\u094b\u0902 \u0915\u0940 \u091c\u093e\u0902\u091a \u0915\u0930\u0947\u0902\u0964",four:"\u0906\u092a\u0915\u094b \u0911\u0928\u0932\u093e\u0907\u0928 \u0905\u092a\u0921\u0947\u091f \u0936\u0941\u0932\u094d\u0915 \u0915\u0947 \u0932\u093f\u090f \u20b9 50 (\u0915\u0947\u0935\u0932 \u092a\u091a\u093e\u0938 \u0930\u0941\u092a\u092f\u0947) \u0915\u093e \u092d\u0941\u0917\u0924\u093e\u0928 \u0915\u0930\u0928\u093e \u0939\u094b\u0917\u093e\u0964 \u0906\u092a \u0915\u094d\u0930\u0947\u0921\u093f\u091f/\u0921\u0947\u092c\u093f\u091f \u0915\u093e\u0930\u094d\u0921 \u092f\u093e \u0928\u0947\u091f \u092c\u0948\u0902\u0915\u093f\u0902\u0917 \u0915\u0947 \u091c\u0930\u093f\u090f \u092d\u0941\u0917\u0924\u093e\u0928 \u0915\u0930 \u0938\u0915\u0924\u0947 \u0939\u0948\u0902\u0964 \u092f\u0939 \u0930\u093e\u0936\u093f \u0905\u092a\u094d\u0930\u0924\u093f\u0926\u0947\u092f \u0939\u0948\u0964 \u092d\u0941\u0917\u0924\u093e\u0928 \u0915\u0947 \u092e\u093e\u092e\u0932\u0947 \u092e\u0947\u0902, \u0906\u092a \u092d\u0941\u0917\u0924\u093e\u0928 \u0915\u093e \u092a\u0941\u0928: \u092a\u094d\u0930\u092f\u093e\u0938 \u0915\u0930 \u0938\u0915\u0924\u0947 \u0939\u0948\u0902\u0964",five:"\u0906\u092a\u0915\u094b \u090f\u0915 \u0938\u0902\u0926\u0930\u094d\u092d \u0915\u0947 \u0930\u0942\u092a \u092e\u0947\u0902 \u090f\u0915 \u0938\u0947\u0935\u093e \u0905\u0928\u0941\u0930\u094b\u0927 \u0938\u0902\u0916\u094d\u092f\u093e (SRN) \u092a\u094d\u0930\u093e\u092a\u094d\u0924 \u0939\u094b\u0917\u0940\u0964 \u0905\u0926\u094d\u092f\u0924\u0928 \u0905\u0928\u0941\u0930\u094b\u0927 \u0915\u094b \u091f\u094d\u0930\u0948\u0915 \u0915\u0930\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u092f\u093e \u092f\u0942\u0906\u0908\u0921\u0940\u090f\u0906\u0908 \u0939\u0947\u0932\u094d\u092a\u0921\u0947\u0938\u094d\u0915 \u0915\u0947 \u0938\u093e\u0925 \u092d\u0935\u093f\u0937\u094d\u092f \u092e\u0947\u0902 \u0915\u093f\u0938\u0940 \u092d\u0940 \u0938\u0902\u091a\u093e\u0930 \u0915\u0947 \u0932\u093f\u090f \u0907\u0938 \u0928\u0902\u092c\u0930 \u0915\u094b \u0938\u0941\u0930\u0915\u094d\u0937\u093f\u0924 \u0930\u0916\u0947\u0902\u0964",six:"\u0906\u092a\u0915\u093e \u0905\u0928\u0941\u0930\u094b\u0927 \u0939\u092e\u093e\u0930\u0947 \u0906\u0902\u0924\u0930\u093f\u0915 \u0917\u0941\u0923\u0935\u0924\u094d\u0924\u093e \u091c\u093e\u0902\u091a \u092a\u094d\u0930\u091a\u093e\u0932\u0915\u094b\u0902 \u0926\u094d\u0935\u093e\u0930\u093e \u0938\u0924\u094d\u092f\u093e\u092a\u093f\u0924 \u0915\u093f\u092f\u093e \u091c\u093e\u090f\u0917\u093e\u0964 \u0907\u0938 \u0938\u094d\u0924\u0930 \u092a\u0930 \u092a\u094d\u0930\u0926\u093e\u0928 \u0915\u093f\u090f \u0917\u090f \u091c\u0928\u0938\u093e\u0902\u0916\u094d\u092f\u093f\u0915\u0940 \u0921\u0947\u091f\u093e \u0915\u094b \u0938\u0939\u093e\u092f\u0915 \u0926\u0938\u094d\u0924\u093e\u0935\u0947\u091c\u093c \u0915\u0947 \u0935\u093f\u0930\u0941\u0926\u094d\u0927 \u091c\u093e\u0902\u091a\u093e \u091c\u093e\u090f\u0917\u093e\u0964 \u092f\u0926\u093f \u0915\u094b\u0908 \u092c\u0947\u092e\u0947\u0932 \u0939\u0948, \u0924\u094b \u0906\u092a\u0915\u0947 \u0905\u0928\u0941\u0930\u094b\u0927 \u0915\u094b \u0905\u0938\u094d\u0935\u0940\u0915\u093e\u0930 \u0915\u093f\u090f \u091c\u093e\u0928\u0947 \u0915\u0940 \u0938\u0902\u092d\u093e\u0935\u0928\u093e \u0939\u0948\u0964",seven:"\u0917\u0941\u0923\u0935\u0924\u094d\u0924\u093e \u091c\u093e\u0902\u091a \u092a\u094d\u0930\u0915\u094d\u0930\u093f\u092f\u093e \u092a\u0942\u0930\u0940 \u0939\u094b\u0928\u0947 \u092a\u0930, \u0906\u092a\u0915\u094b \u092a\u0930\u093f\u0923\u093e\u092e \u0915\u0947 \u0924\u094c\u0930 \u092a\u0930  \u0928\u093e\u092e\u093e\u0902\u0915\u0928 \u0906\u0908\u0921\u0940 \u092f\u0941\u0915\u094d\u0924 \u090f\u0915 \u090f\u0938\u090f\u092e\u090f\u0938 \u0938\u0942\u091a\u0928\u093e \u092a\u094d\u0930\u093e\u092a\u094d\u0924 \u0939\u094b\u0917\u0940\u0964",eight:"\u0939\u092e\u093e\u0930\u0947 \u092c\u0948\u0915\u090f\u0902\u0921 \u0938\u0949\u092b\u093c\u094d\u091f\u0935\u0947\u092f\u0930 \u0926\u094d\u0935\u093e\u0930\u093e \u0915\u0941\u091b \u0914\u0930 \u0938\u0924\u094d\u092f\u093e\u092a\u0928 \u0915\u0947 \u092c\u093e\u0926, \u0906\u092a\u0915\u0947 \u0905\u0928\u0941\u0930\u094b\u0927 \u0915\u0940 \u092a\u094d\u0930\u0915\u094d\u0930\u093f\u092f\u093e \u092a\u0942\u0930\u0940 \u0939\u094b \u091c\u093e\u090f\u0917\u0940\u0964 \u0906\u092a\u0915\u094b \u092a\u0930\u093f\u0923\u093e\u092e \u0915\u0947 \u0924\u094c\u0930 \u092a\u0930  \u090f\u0915 \u090f\u0938\u090f\u092e\u090f\u0938 \u0938\u0942\u091a\u0928\u093e \u092a\u094d\u0930\u093e\u092a\u094d\u0924 \u0939\u094b\u0917\u0940\u0964",nine:"\u0906\u092e \u0924\u094c\u0930 \u092a\u0930 \u0905\u0926\u094d\u092f\u0924\u0928 \u0905\u0928\u0941\u0930\u094b\u0927 90% \u0938\u0947\u0935\u093e \u092e\u093e\u0928\u0915\u094b\u0902 \u0915\u0947 \u0938\u093e\u0925 30 \u0926\u093f\u0928\u094b\u0902 \u0915\u0947 \u092d\u0940\u0924\u0930 \u092a\u0942\u0930\u093e \u0915\u093f\u092f\u093e \u091c\u093e\u0924\u093e \u0939\u0948\u0964 \u0939\u093e\u0932\u093e\u0902\u0915\u093f, \u0939\u092e \u0905\u092a\u0928\u0947 \u0938\u093f\u0938\u094d\u091f\u092e \u092e\u0947\u0902 \u0938\u0941\u0927\u093e\u0930 \u0915\u0930 \u0930\u0939\u0947 \u0939\u0948\u0902 \u0914\u0930 \u0905\u0927\u093f\u0915 \u0938\u0902\u092d\u093e\u0935\u0928\u093e \u0939\u0948 \u0915\u093f \u0906\u092a\u0915\u0947 \u0905\u0928\u0941\u0930\u094b\u0927 \u0915\u094b \u092c\u0924\u093e\u090f \u0917\u090f \u0938\u092e\u092f \u0938\u0947 \u092a\u0939\u0932\u0947 \u0939\u0940 \u0905\u092a\u0921\u0947\u091f \u0915\u0930 \u0926\u093f\u092f\u093e \u091c\u093e\u090f\u0964 \u0906\u092a myaadhaar.uidai.gov.in \u092a\u0930 \u091c\u093e\u0915\u0930 \u0930\u093f\u0915\u094d\u0935\u0947\u0938\u094d\u091f \u092a\u094d\u0930\u094b\u0938\u0947\u0938\u093f\u0902\u0917 \u0915\u0940 \u0938\u094d\u0925\u093f\u0924\u093f \u0915\u0940 \u091c\u093e\u0902\u091a \u0915\u0930 \u0938\u0915\u0924\u0947 \u0939\u0948\u0902\u0964"}},updateSelectPage:{title:"\u0906\u0927\u093e\u0930 \u0911\u0928\u0932\u093e\u0907\u0928 \u0905\u092a\u0921\u0947\u091f \u0915\u0930\u0947\u0902",selectFields:{smallDescription:"\u0905\u092a\u0921\u0947\u091f \u0915\u0930\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0906\u0927\u093e\u0930 \u0921\u0947\u091f\u093e \u092b\u093c\u0940\u0932\u094d\u0921 \u091a\u0941\u0928\u0947\u0902\u0964",longDescription:"\u0905\u0926